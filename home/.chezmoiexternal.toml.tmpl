{{ if .personal -}}
[".oh-my-zsh"]
    type = "archive"
    url = "https://github.com/ohmyzsh/ohmyzsh/archive/master.tar.gz"
    exact = true
    stripComponents = 1
    refreshPeriod = "168h"
[".oh-my-zsh/custom/plugins/zsh-syntax-highlighting"]
    type = "archive"
    url = "https://github.com/zsh-users/zsh-syntax-highlighting/archive/master.tar.gz"
    exact = true
    stripComponents = 1
    refreshPeriod = "168h"
[".oh-my-zsh/custom/plugins/autoswitch_virtualenv"]
    type = "archive"
    url = "https://github.com/MichaelAquilina/zsh-autoswitch-virtualenv/archive/master.tar.gz"
    exact = true
    stripComponents = 1
    refreshPeriod = "168h"
[".garyj/font-monaspace"]
    type = "git-repo"
    url = "https://github.com/githubnext/monaspace.git"
    refreshPeriod = "168h"
{{ end -}}
{{- if eq .chezmoi.os "darwin" -}}
[".hammerspoon/Spoons/SkyRocket.spoon"]
    type = "git-repo"
    url = "https://github.com/oizik/dotfiles_SkyRocket.spoon.git"
    refreshPeriod = "168h"
{{ end -}}

